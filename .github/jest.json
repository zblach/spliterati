{
  "problemMatcher": [
    {
      "owner": "yarn-test",
      "pattern": [
        {
          "regexp": "([^\\s].*)$",
          "file": 1
        },
        {
          "regexp":"^::(error) file=([^,].*)line=(\\d+),col=(\\d+)::(.*)$",
          "severity": 1,
          "file": 2,
          "column": 3,
          "message": 4,
          "loop": true
        }
      ]
    }
  ]
}