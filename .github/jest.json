{
  "problemMatcher": [
    {
      "owner": "yarn-test",
      "pattern": [
        {
          "regexp": "([^\\s].*)$",
          "file": 1
        },
        {
          "regexp":"^\"\\s+(\\d+):(\\d+):(.*)\"$",
          "line": 1,
          "column": 2,
          "message": 3,
          "loop": true
        }
      ]
    }
  ]
}